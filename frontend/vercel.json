{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "env": {
        "NEXT_PUBLIC_API_URL": {
            "description": "Backend API URL (e.g. https://busiq-api.railway.app/api/v1)"
        },
        "NEXT_PUBLIC_WS_URL": {
            "description": "WebSocket URL (e.g. wss://busiq-api.railway.app/api/v1/ws/live)"
        },
        "NEXT_PUBLIC_MAPBOX_TOKEN": {
            "description": "Mapbox GL access token"
        }
    }
}